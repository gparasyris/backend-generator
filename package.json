{
  "name": "generator-sventech",
  "version": "0.0.2",
  "description": "This is a scaffold for backend NodeJS server developed by sventech.io accounts",
  "homepage": "https://github.com/gparasyris/backend-generator",
  "author": {
    "name": "George Parasyris",
    "email": "gparasyris@sventech.io",
    "url": "https://github.com/gparasyris/backend-generator"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "nodejs",
    "modules",
    "providers",
    "scaffold",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.4.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "asyncawait": "^1.0.7",
    "chalk": "^2.1.0",
    "just-extend": "4.0.0",
    "merge": "1.2.1",
    "pluralize": "^7.0.0",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "https://gparasyris@github.com/gparasyris/back-end-challenge.git",
  "license": "MIT"
}
